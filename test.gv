BEG_G {   graph_t sg;   double delta, y, maxy = 0;   string ur;   for (sg=fstsubg($);sg;sg=nxtsubg(sg)) {     if (sg.name != "cluster*") continue;     y = (double)yOf(urOf(sg.bb));     maxy = MAX(y,maxy);   }   for (sg=fstsubg($);sg;sg=nxtsubg(sg)) {     if (sg.name != "cluster*") continue;     ur = urOf(sg.bb);     y = (double)yOf(ur);     if (y < maxy) {       sg.bb = sprintf ("%s,%s,%.03f",llOf(sg.bb),xOf(ur),maxy);       if (sg.lp != "") {         delta = maxy-y;         y = (double)yOf(sg.lp);         sg.lp = sprintf ("%s,%.03f",xOf(sg.lp),y+delta);       }     }   } }